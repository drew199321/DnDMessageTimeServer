{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "no-console": "off",
      "linebreak-style": 0,
      "max-len": ["error", {
        "code": 125,
        "ignoreComments": true,
        "ignoreTrailingComments": true,
        "ignoreUrls": true
      }]
    }
}
